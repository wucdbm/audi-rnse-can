{
    "name": "wucdbm/audi-rnse-can",
    "description": "Audi RNS-E CANBus bindings",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "ext-canbus": "*",
        "symfony/console": "~7.2",
        "symfony/process": "~7.2",
        "guzzlehttp/guzzle": "~7.9",
        "symfony/dotenv": "~7.2"
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "scripts": {
        "cs": "PHP_CS_FIXER_IGNORE_ENV=1 ./bin/php-cs-fixer fix --config=./.php-cs-fixer.dist.php"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wucdbm\\AudiRnseCan\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Martin Kirilov"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "~2.1",
        "phpstan/phpdoc-parser": "~2.1",
        "ergebnis/phpstan-rules": "~2.8",
        "phpro/grumphp": "~v2.12",
        "nikic/php-parser": "~v5.4",
        "php-parallel-lint/php-parallel-lint": "~1.4"
    }
}
